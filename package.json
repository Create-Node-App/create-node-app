{
  "name": "playground.js",
  "version": "1.0.0",
  "description": "A basic template that consists of the essential elements that are required to start building a React application using Redux",
  "scripts": {
    "build": "webpack",
    "build:dev": "yarn build --mode development",
    "build:dev:watch": "yarn build:dev --watch",
    "build:dev:bundleanalyze": "yarn build:dev --addons=bundleanalyze",
    "build:dev:bundlebuddy": "yarn build:dev --addons=bundlebuddy",
    "build:prod": "yarn build -p",
    "build:prod:watch": "yarn build:prod --watch",
    "build:prod:bundleanalyze": "yarn build:prod --addons=bundleanalyze",
    "build:prod:bundlebuddy": "yarn build:prod --addons=bundlebuddy",
    "docker:dev": "yarn docker:build && yarn docker:start",
    "docker:build": "docker build -f docker/Dockerfile -t react-redux-webpack-starter .",
    "docker:start": "docker run --rm -it --network host -v $PWD:/usr/src/app react-redux-webpack-starter",
    "lint": "eslint .; exit 0",
    "lint:fix": "eslint . --fix; exit 0",
    "reducer:generate": "reducer-maker -w src",
    "reducer:help": "reducer-maker --help",
    "serve:dev": "webpack-dev-server --mode development",
    "serve:dev:dashboard": "webpack-dashboard webpack-dev-server -- --mode development --addons=dashboard",
    "serve:prod": "yarn build:prod && live-server ./dist",
    "test": "echo \"No tests available\"; exit 0",
    "webpack-defaults": "webpack-defaults"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ulises-jeremias/playground.js.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/moment-timezone": "^0.5.9",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.6.1",
    "file-loader": "^1.1.11",
    "less": "^2.7.2",
    "less-loader": "^4.1.0",
    "live-server": "^1.2.0",
    "node-sass": "^4.7.2",
    "reducer-maker": "^0.0.8",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack-hot-middleware": "^2.22.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bundle-buddy-webpack-plugin": "^0.3.0",
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.5.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "i18next": "^11.4.0",
    "i18next-browser-languagedetector": "^2.2.2",
    "i18next-xhr-backend": "^1.5.1",
    "inject-html-webpack-plugin": "^0.0.39",
    "js-cookie": "^2.2.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.21.0",
    "moment-timezone": "^0.5.14",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "react": "^16.4.1",
    "react-barcode": "^1.3.4",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.3.4",
    "react-i18next": "^7.9.0",
    "react-mixin": "^4.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "react-timer-mixin": "^0.13.4",
    "redux": "^4.0.0",
    "redux-cookie": "^0.5.9",
    "redux-devtools": "^3.4.1",
    "redux-devtools-chart-monitor": "^1.7.0",
    "redux-devtools-dispatch": "^2.2.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-filterable-log-monitor": "^0.8.0",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-devtools-multiple-monitors": "^1.0.1",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-slider-monitor": "^2.0.0-1",
    "redux-thunk": "^2.3.0",
    "semantic-ui-less": "^2.3.3",
    "semantic-ui-react": "^0.82.0",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "underscore": "^1.9.1",
    "webpack": "^4.16.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-cli": "^3.0.8",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-merge": "^4.1.1"
  }
}
